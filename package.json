    "tsup": "8.3.0",
    "typescript": "5.7.2"
  },
  "pnpm": {
    "overrides": {
      "inquirer": "^9.2.8",
      "whatwg-url": "^14.0.0"
    },
    "patchedDependencies": {
      "import-in-the-middle": "patches/import-in-the-middle.patch",
      "@opentelemetry/instrumentation": "patches/@opentelemetry__instrumentation.patch"
